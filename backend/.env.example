# LLM Provider
WIRO_API_KEY=your_wiro_api_key
WIRO_API_SECRET=your_wiro_api_secret
LLM_MODEL=gpt-5-2
WIRO_BASE_URL=https://api.wiro.ai
WIRO_REASONING=medium
WIRO_WEB_SEARCH=false
WIRO_VERBOSITY=medium
WIRO_POLL_INTERVAL_SECONDS=1.0
WIRO_POLL_TIMEOUT_SECONDS=90.0
WIRO_HTTP_TIMEOUT_SECONDS=60.0

# Database (SQLite default for dev; use PostgreSQL for production)
# DATABASE_URL=postgresql+asyncpg://dotshub:dotshub_secret@localhost:5432/dotshub
DATABASE_URL=sqlite+aiosqlite:///./dotshub.db

# Supabase (for feedback & dashboard data)
SUPABASE_URL=https://xxxx.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key
SUPABASE_DB_URL=postgresql://postgres:password@db.xxxx.supabase.co:5432/postgres
ADMIN_API_KEY=change_me

# Redis
REDIS_URL=redis://localhost:6379/0

# Facility discovery
FACILITY_DISCOVERY_ENABLED=false
FACILITY_DISCOVERY_TIMEOUT_SECONDS=2.5

# App
APP_ENV=development
DEBUG=true
CORS_ORIGINS=["http://localhost:8081","http://localhost:19006","http://localhost:3000"]
